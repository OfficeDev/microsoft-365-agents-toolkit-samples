{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.17/MicrosoftTeams.schema.json",
    "manifestVersion": "1.17",
    "version": "1.0.0",
    "id": "${{TEAMS_APP_ID}}",
    "developer": {
        "name": "Alexander Gobeil",
        "websiteUrl": "https://44.is-a.dev/",
        "privacyUrl": "https://44.is-a.dev/",
        "termsOfUseUrl": "https://44.is-a.dev/"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "DMJv2",
        "full": "Do My Job Version 2.0"
    },
    "description": {
        "short": "Teams interface application for DMJ",
        "full": "Teams interface application for Do My Job"
    },
    "accentColor": "#00FFFF",
    "bots": [
        {
            "botId": "${{BOT_ID}}",
            "scopes": [
                "personal",
                "team",
                "groupChat"
            ],
            "supportsFiles": false,
            "isNotificationOnly": false
        }
    ],
    "composeExtensions": [],
    "configurableTabs": [],
    "staticTabs": [],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": []
}